# Backend Environment Variables for Render

# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=oftalmolaser-api.onrender.com

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://oftalmolaser.vercel.app,https://oftalmolaser.com

# Database (Render PostgreSQL - oftalmolaserdb)
DATABASE_URL=postgresql://user:password@host:port/oftalmolaserdb

# Security
CSRF_TRUSTED_ORIGINS=https://oftalmolaser.vercel.app

# Static Files
STATIC_URL=/static/
MEDIA_URL=/media/

# ML Model Settings
ML_MODEL_VERSION=v2.0